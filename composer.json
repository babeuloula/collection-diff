{
    "name": "wizaplace/collection-diff",
    "description": "Collection Diff allows you to compare 2 data collections to find operations to do: CREATE, UPDATE or DELETE data.",
    "type": "library",
    "homepage": "https://github.com/wizaplace/collection-diff",
    "require": {
        "php": "^7.1",
        "symfony/serializer": "^2.7||^3.0||^4.0"
    },
    "suggest": {
        "symfony/framework-bundle": "Needed to use Collection Diff's bridge",
        "friendsofphp/php-cs-fixer": "Needed for syntax ^2.14",
        "phpstan/phpstan": "Needed for syntax ^0.11.2",
        "phpunit/phpunit": "Needed for tests ^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wizaplace",
            "email": "dev@wizaplace.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Wizaplace\\CollectionDiff\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wizaplace\\Test\\CollectionDiff\\": "tests/"
        }
    }
}
