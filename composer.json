{
    "name": "babeuloula/collection-diff",
    "description": "Collection Diff allows you to compare 2 data collections to find operations to do: CREATE, UPDATE or DELETE data.",
    "type": "library",
    "homepage": "https://github.com/babeuloula/collection-diff",
    "require": {
        "php": "^7.1|",
        "symfony/serializer": "^2.7||^3.0||^4.0||^5.0||^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.11.2",
        "phpunit/phpunit": "^8.0"
    },
    "suggest": {
        "symfony/framework-bundle": "Needed to use Collection Diff's bridge"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wizaplace",
            "email": "dev@wizaplace.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Wizaplace\\CollectionDiff\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wizaplace\\Test\\CollectionDiff\\": "tests/"
        }
    }
}
